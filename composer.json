{
    "name": "born-to-ride/booking-plugin",
    "description": "WordPress Plugin for Born to Ride travel booking system",
    "type": "wordpress-plugin",
    "license": "proprietary",
    "version": "1.0.199",
    "authors": [
        {
            "name": "Born to Ride",
            "email": "info@borntoride.it"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.2"
    },
    "autoload": {
        "psr-4": {
            "BTR\\": "includes/"
        },
        "files": [
            "includes/helpers/btr-meta-helpers.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "composer dump-autoload -o"
        ],
        "post-update-cmd": [
            "composer dump-autoload -o"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "classmap-authoritative": false
    }
}